Vendedor.java
import java.time.LocalDate;

public class Vendedor {

    public String nome;
    public String idade;
    public String telefone;
    public String rg;
    public LocalDate dataDeNascimento;
    public LocalDate dataDeAdmissao;
    public float salario;
    public float comissao;
    
}
